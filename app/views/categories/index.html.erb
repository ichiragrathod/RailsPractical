<h1>Categories </h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
  <% @categories.each do |category| %>
    <tr>
      <td> <%= category.id %> </td>
      <td> <%= category.category_name %> </td>
    </tr>
  <% end %>   
  </tbody>
</table>
